#include <iostream>

using namespace std;

int main() 
{
	string funcionarios[10][5];
	
	for(int i=0;i < 5;i++)
	{
		cout << "Informe o nome " << i+1 << endl;
		cin >> funcionarios[i][0];
		
		cout << "Informe o RA " << i+1 << endl;
		cin >> funcionarios[i][1];
		
		cout << "Informe a nota " << i+1 << endl;
		cin >> funcionarios[i][2];
		
		cout << "Informe a nota " << i+1 << endl;
		cin >> funcionarios[i][3];
		
		cout << "Informe a nota " << i+1 << endl;
		cin >> funcionarios[i][4];
		
		cout << "-------------------------------------------------------------------------------------------------" << endl;
	}
	
	for(int i=0; i < 5;i++)
	{
		cout << funcionarios[i][0] << "|" << funcionarios[i][1] << << funcionarios[i][2] << funcionarios[i][3] << funcionarios[i][4] << endl;
	}
	
	return 0;
}
