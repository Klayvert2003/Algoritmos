#include <iostream>
#include <locale>

using namespace std;

int main() 
{
	setlocale (LC_ALL, ("Portuguese"));
	
	int v[4], r;
	
	cout << "Insira quatro valores no vetor: " << endl;
	
	for(int i=0;i < 4;i++)
	{
		cin >> v[4];
		r = v[4] + v[4] + v[4] + v[4];	
	}
		cout << "A soma dos valores inseridos e: " << r << endl;
	
	return 0;
}
